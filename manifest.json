{
    "name" : "Title Picker",
    "description" : "returns the title of the chrome tab",
    "version" : "1.0",
    "manifest_version" : 3,
    "background" :{
        "service_worker" : "background.js"
    },
    "action" :{
        "default_popup" :"popup.html"
    },
    "icons" : {
        "16" : "/images/31859970-1.png",
        "32" : "/images/31859970-2.png",
        "48" : "/images/31859970-3.png",
        "128" : "/images/31859970-4.png"
    },
    "permissions": ["storage", "declarativeContent", "activeTab", "scripting","tabs"]

}